			</define>
			<define>
				<name>include_jpeg_encoder</name>
				<value type="bool">True</value>
			</define>
			<define>
				<name>include_edge_video_system</name>
				<value type="bool">True</value>
			</define>
			<define>
				<name>include_florian</name>
				<value type="bool">True</value>
			</define>
